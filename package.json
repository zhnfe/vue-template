{
    "name": "vue-template",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "build": "vite build && sh scripts/version.sh",
        "type-check": "vue-tsc -b",
        "lint": "eslint --fix",
        "lint-only": "eslint --quiet",
        "postinstall": "simple-git-hooks"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=10.0.0"
    },
    "dependencies": {
        "@material-symbols/svg-400": "^0.38.0",
        "es-toolkit": "^1.44.0",
        "naive-ui": "^2.43.2",
        "pinia": "^2.3.1",
        "sortablejs": "^1.15.6",
        "tailwindcss": "^4.1.18",
        "vue": "latest",
        "vue-draggable-plus": "^0.6.1",
        "vue-router": "latest"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^5.7.1",
        "@tailwindcss/vite": "^4.1.18",
        "@tsconfig/node22": "^22.0.5",
        "@types/node": "^22.19.7",
        "@types/sortablejs": "^1.15.9",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vitejs/plugin-vue-jsx": "^5.1.3",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.7.0",
        "baseline-browser-mapping": "^2.9.19",
        "eslint": "^9.39.2",
        "eslint-plugin-vue": "^10.7.0",
        "lint-staged": "^16.2.7",
        "npm-run-all2": "^6.2.6",
        "simple-git-hooks": "^2.13.1",
        "typescript": "~5.9.3",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^7.3.1",
        "vue-tsc": "^3.2.4"
    },
    "lint-staged": {
        "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --fix"
    },
    "simple-git-hooks": {
        "pre-commit": "npx run-p lint type-check"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "simple-git-hooks",
            "vue-demi"
        ],
        "ignoredBuiltDependencies": [
            "@tailwindcss/oxide"
        ]
    }
}